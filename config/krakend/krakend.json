{
    "$schema": "https://www.krakend.io/schema/v2.5/krakend.json",
    "version": 3,
    "name": "my api gateway",
    "port": 8080,
    "cache_ttl": "3600s",
    "timeout": "3s",
    "extra_config": {
      "telemetry/logging": {
        "level":  "DEBUG",
        "prefix": "[KRAKEND]",
        "syslog": false,
        "stdout": true
      },
      "telemetry/metrics": {
        "collection_time": "60s",
        "proxy_disabled": false,
        "router_disabled": false,
        "backend_disabled": false,
        "endpoint_disabled": false,
        "listen_address": ":8090"
      }
    },
    "endpoints": [
        {
            "endpoint": "/homest",
            "method": "GET",
            "backend": [
                {
                    "url_pattern": "/service-two/messages",
                    "sd": "dns",
                    "is_collection": true,
                    "sd_scheme": "http",
                    "encoding": "json",
                    "host": [
                        "service-two.service.consul"
                    ],
                    "disable_host_sanitize": true
                }
            ]
        },
        {
            "endpoint": "/homeso",
            "method": "GET",
            "backend": [
                {
                    "url_pattern": "/service-one/messages",
                    "sd": "dns",
                    "is_collection": true,
                    "sd_scheme": "http",
                    "encoding": "json",
                    "host": [
                        "service-one.service.consul"
                    ],
                    "disable_host_sanitize": true
                }
            ]
        }
    ]
}